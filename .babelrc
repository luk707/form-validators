{
  "presets": ["env", "react"],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "^~$": "./src/index.js",
          "^~/(.+)": "./src/\\1",
          "^%$": "./docs/index.js",
          "^%/(.+)": "./docs/\\1"
        }
      }
    ]
  ]
}
